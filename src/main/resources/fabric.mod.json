  "mixins": [
    "carpet-fixes.mixins.json"
  ],
  "accessWidener" : "carpet-fixes.accesswidener",
  "depends": {
    "minecraft": ">=1.19",
    "fabricloader": ">=0.11.3",
    "carpet": ">=1.4.79",
    "java": ">=17",
    "conditional-mixin": "*"
  },
  "custom": {
    "mc-publish": {
      "curseforge": 529720,
      "modrinth": "7Jaxgqip"
    },
    "modmenu": {
      "links": {
        "modmenu.discord": "https://discord.gg/mezndgtD83"
      },
      "parent": "carpet"
    }
  }
}
